    // ** Dictionary Completion **

    "dictCompletion.addSpaceAfterCompletion": true,
    "dictCompletion.collectWordsFromCurrentFile": false,
    "dictCompletion.externalUserDictFiles":[],
    "dictCompletion.leastNumOfChars": 3,
    "dictCompletion.programmingLanguage": true,
    "dictCompletion.useExternalUserDictFile": true,
    "dictCompletion.userDictionary": ["whitespaces"],

    // ** DIFF EDITOR **

    "diffEditor.codeLens": false,
    "diffEditor.diffAlgorithm": "advanced",
    "diffEditor.experimental.showEmptyDecorations": true,
    "diffEditor.experimental.showMoves": false,
    "diffEditor.hideUnchangedRegions.contextLineCount": 3,
    "diffEditor.hideUnchangedRegions.enabled": false,
    "diffEditor.hideUnchangedRegions.minimumLineCount": 3,
    "diffEditor.hideUnchangedRegions.revealLineCount": 20,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.maxComputationTime": 5000,
    "diffEditor.maxFileSize": 50,
    "diffEditor.renderIndicators": true,
    "diffEditor.renderMarginRevertIcon": true,
    "diffEditor.renderSideBySide": true,
    "diffEditor.renderSideBySideInlineBreakpoint": 900,
    "diffEditor.useInlineViewWhenSpaceIsLimited": true,
    "diffEditor.wordWrap": "inherit",

    // ** EDITOR **

		// Accept suggestions using commit characters
  	// Commit characters = semicolon for Javascript, Enter_key for text etc.
  	// Alternative only Tab_key
    "editor.acceptSuggestionOnCommitCharacter": false, // https://youtu.be/Hmy-zBfpmdk
    
		// Tab + Enter both accept suggestions
		// Decided when to accept suggestion and when to enter new line based on context
		"editor.acceptSuggestionOnEnter": "smart", // https://youtu.be/GMM2zZzNFRs
		
		// 
    "editor.accessibilityPageSize": 1, // 

		// Supportive feature for speech synthesizers or braille displays.
    "editor.accessibilitySupport": "off", // https://youtu.be/Z779xMbSszg
    
		// Decides when to autoclose brackets
		"editor.autoClosingBrackets": "beforeWhitespace", // https://youtu.be/VICsBFsJT1s

    "editor.autoClosingComments": "languageDefined",
		
    "editor.autoClosingDelete": "auto",

		// Type over a closing parenthesis only if automatically generated
    "editor.autoClosingOvertype": "auto", // https://youtu.be/SucNhPsTWH0

		// Decides when to autoclose Quotes 
		"editor.autoClosingQuotes": "beforeWhitespace", // https://youtu.be/l5lFt4YFdcg
    
		// Controls whether to auto adjust indentation when typing/pasting/moving lines.
		"editor.autoIndent": "full", // https://youtu.be/I1W7ExCYBz4

		// Automatically surround selections
		// [ASK FOR ALWAYS]
    "editor.autoSurround": "languageDefined", // https://youtu.be/fpnPLCyqsKQ

    // Enable bracket colorization
		"editor.bracketPairColorization.enabled": true, // https://youtu.be/4UXlkUo-emY
    
		
		"editor.bracketPairColorization.independentColorPoolPerBracketType": false,
    
		"editor.codeActionsOnSave": {

			// Apply linter proposed automatic fixes on format.
			"source.fixAll": "always", // https://youtu.be/gj11Z9QIfJU
			
			"source.organizeImports": "explicit",

		},

    "editor.codeActionWidget.includeNearbyQuickFixes": true,
		
		
    "editor.codeActionWidget.showHeaders": true,

		// ** CODE LENS ** 

		// Disable showing codelens references inside Editor
    "editor.codeLens": false, // https://youtu.be/JNT9P5u5nIA
		
		"editor.codeLensFontFamily": "",
		
    "editor.codeLensFontSize": 0,
		
		// ** COLOR DECORATORS ** 

		// Show Inline color indicators that trigger the color pallet on click
		"editor.colorDecorators": true, // https://youtu.be/YVyZF8Os6fg

    "editor.colorDecoratorsActivatedOn": "clickAndHover",
		
    "editor.colorDecoratorsLimit": 500,

    "editor.columnSelection": false,
    
		// ** EDITOR COMMENTS ** 

		// 
		"editor.comments.ignoreEmptyLines": true, // 

		// When creating a Comment, add a space after it.
    "editor.comments.insertSpace": true, //
    
		// Copying a piece of code to a different application, includes syntax.
		"editor.copyWithSyntaxHighlighting": true, // https://youtu.be/5d8ubUKpF-A

		// Sets cursor render animation to Smooth
    "editor.cursorBlinking": "smooth", // https://youtu.be/q1wAnW_yFDU - https://youtu.be/Hms3gUBKaVQ
		
		// Enable Smoother Cursor Animation
    "editor.cursorSmoothCaretAnimation": "on", // https://youtu.be/c-6AzCZ6mjw - https://youtu.be/FCUi_dRU0tY
    
		// Set Cursor Style - underline instead of line
		"editor.cursorStyle": "underline-thin", // https://youtu.be/Nki8tVQ82_8
		
		// Move the window Up or Down when the cursor is exactly X lines from the top/bottom
		"editor.cursorSurroundingLines": 25, // https://youtu.be/PQAuhE6bBEc

		// Decided when "cursor surrounding lines" policy should be enforced
  	// Default = don't move view with cursor , but move with keyboard
  	// All = move with both - All causes view to move when you click (not wanted)
  	// All also causes text to be selected as you move to a different view - bug
		"editor.cursorSurroundingLinesStyle": "default", // 
		
		// Works only when "cursorStyle" is set to "line"
    "editor.cursorWidth": 2, // https://youtu.be/-S-clq9jvyk
    
		
		"editor.defaultColorDecorators": false,
    
		"editor.defaultFoldingRangeProvider": null,
    
		"editor.defaultFormatter": null,
    
		"editor.definitionLinkOpensInPeek": false,
    
		// 
		"editor.detectIndentation": false,  // https://www.youtube.com/watch?v=vGsXNImZz4A

		// Disable "Drag and Drop" behavior inside the Editor
    "editor.dragAndDrop": false, // https://youtu.be/dJiZ1Rv2s5I
		
		"editor.dropIntoEditor.enabled": true,
    "editor.dropIntoEditor.showDropSelector": "afterDrop",

    // Copying without a selection, defaults to current cursor line
		"editor.emptySelectionClipboard": true, // https://youtu.be/NAokkP8jJkU
    
		
		"editor.experimental.asyncTokenization": true,
    "editor.experimental.asyncTokenizationLogging": false,
    "editor.experimental.asyncTokenizationVerification": false,
    "editor.experimental.dropIntoEditor.defaultProvider": {},
		
		// Render whitespace characters as SVG icons
    "editor.experimentalWhitespaceRendering": "svg", // 

		// Scrolling while holding Alt - multiplies scroll speed x5
		"editor.fastScrollSensitivity": 5, // https://youtu.be/keOO910u9UI
		
		// Disable Scrolling beyond first line of document
		"editor.find.addExtraSpaceOnTop": false, // https://youtu.be/6B-P0khHW_c
		
		// Never Search inside Selection
		"editor.find.autoFindInSelection": "never", // https://youtu.be/EzGDiZXMgXg
    
		// MacOS Specific
		"editor.find.globalFindClipboard": "true", // https://youtu.be/D2DAft6SN0w

		// During Search, don't jump to matches unless triggered.
		"editor.find.cursorMoveOnType": false, // https://youtu.be/gnyWgbogJwk
    
		
		"editor.find.loop": true,

