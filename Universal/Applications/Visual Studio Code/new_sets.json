    "audioCues.chatRequestSent": "off",
    "audioCues.chatResponsePending": "auto",
    "audioCues.chatResponseReceived": "off",
    "audioCues.clear": "off",
    "audioCues.debouncePositionChanges": false,
    "audioCues.diffLineDeleted": "auto",
