  "outline.problems.enabled": false,

  // Disable Colors in Problems inside Outline
  // https://youtu.be/d4m4gALs7GQ
  "outline.problems.colors": false,

  // Disable problem badges inside Outline
  // https://youtu.be/TCrT6DHg2io
  "outline.problems.badges": false,

  // Show icons inside outline
  "outline.icons": true,

  // ** FORMATTING **

  // YAPF formatter
  "yapf.showNotifications": "always",
  "yapf.importStrategy": "fromEnvironment",

  // ** EXTENSIONS **

  // When opening a new file, don't pop-up recommended extensions
  // https://www.youtube.com/watch?v=HmYhlBmCvzU
  "extensions.ignoreRecommendations": true,

  // Disable Automatically check for new updates
  // https://www.youtube.com/watch?v=r8XWk379Wsc
  "extensions.autoCheckUpdates": false,

  // Disable Auto update extensions
  // https://www.youtube.com/watch?v=Zs6eaSK8tFs
  "extensions.autoUpdate": false,

  // When navigating away, to files, not other editors, close Extension details view automatically
  // https://www.youtube.com/watch?v=h9AWCCjlhLQ
  "extensions.closeExtensionDetailsOnViewChange": true,

  // Allows extensions to open URLs without prompt
  // https://www.youtube.com/watch?v=JWa5sdWls4U
  "extensions.confirmedUriHandlerExtensionIds": [],

